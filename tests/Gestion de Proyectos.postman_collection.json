							"    console.log(res)\r",
							"});\r",
							"//THEN EL CODIGO HTTP RECIBIDO DEBE SER 201 \r",
							"pm.test(\"Status code is 201\", function(){\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"//AND EL PROYECTO ES CREADO\r",
							"pm.test(\"Proyecto creado\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.mensaje).to.eql(\"Proyecto creado\");\r",
							"});"
